    "bcryptjs": "2.4.3",
    "class-validator": "0.13.0",
    "date-fns": "2.29.3",
    "dotenv": "16.0.3",
    "fastify": "4.15.0",
    "graphql": "16.6.0",
    "mercurius": "12.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.0",
